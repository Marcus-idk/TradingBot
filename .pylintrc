[MAIN]
ignore-paths =
    .venv,
    .ruff_cache,
    .pytest_cache,
    .vscode,
    .claude,
    tempfiles,
    docs,
    build,
    dist,
    .mypy_cache,
    .hypothesis,
    .*\.egg-info

ignore-patterns =
    __pycache__,
    .*\.pyc$,
    .*\.pyo$

[MESSAGES CONTROL]
disable = all
enable =
    cyclic-import
